<div id="header">
	<div class="back">Books</div>
	<h1>Check Out</h1>
</div>

<div class="check">

{{#each this}}
	{{#if cover_image}}
		<div class="title-art">
			<img src="{{cover_image}}">
		</div>

	{{else}}
	
		<div class="no-art">
			<div class="no-icon"></div>
		</div>

{{/if}}

<div class="title-info">
	<h2 class="truncate-three">{{title}}</h2>
	<h3 class="truncate">{{author}}</h3>
	{{!-- <h4>ISBN: {{ISBN}}</h4> --}}
	<p>{{quantity_available}} Available</p>
	<div id="checkOut" class="check-btn button disabled">Check Out</div> {{!-- on name select remove .disabled and add .primary-fill --}}
</div>
{{/each}}

<div class="clearfix"></div>

<div id="wrapper" class="check-wrap">
	<div class="name-header">Pick your name</div>
	<div id="scroller" class="students">
	</div> {{!-- /#scroller --}}
</div> {{!-- /#wrapper --}}

